1)
#include <iostream>
using namespace std;

int main() {
	cout << "Vvedite celoe chislo p: ";
	int p;
	cin >> p;
	cout << "Vvedite chislo q: ";
	int q;
	cin >> q;
					if (p>0)
{
		if (p%q==0)
	{
		cout << "Chislo delitsya nacelo: " << p/q << "\n";
	}
			else
		{
			cout << "Chiclo ne delitsya nacelo " << "\n";
		}
}



	else
{
		cout << "P<0" << "\n";
}

return 0;
}



2)
#include <iostream>
using namespace std;

int main () {
cout << "Vvedite chislo a: ";
int a;
cin >> a;

if (a<9&&a%3==0) {
cout<< "A<9||a%3==0"<< "\n";
return 0;
}

else {
cout << "A>9||a%3!=0" << "\n";
return 0;
}

return 0;
}


3)
#include <iostream>
using namespace std;



int main()
{


int a;
cout << "Vvedite a: ";
cin >> a;


	if (a>0&&a>99&&a<1000)
{
	cout << "true" << "\n";
}

	else
{
	cout << "false" << "\n";
}


return 0;

}



4)
#include <iostream>
using namespace std;


int main()
{

	int a;
	cout << "Vvedite a: ";
	cin >> a;


	if (a>0||a%2==0)
{
	a++;
	cout << "-> " << a*5 << "\n";
}


	else
{
	cout << "-> " << a*5 << "\n";
}


return 0;

}




5)
#include <iostream>
using namespace std;


int main()
{

	int a;
	cout << "Vvedite a: ";
	cin >> a;


	if (a%2==0||(a%10)==0)
{
	a+=2;
	cout << "-> " << a << "\n";
}


	else
{
	cout << "-> " << a-10 << "\n";
}


return 0;

}
